{
  "name": "my-qwik-empty-starter",
  "description": "App with Routing built-in ready to create your app",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/fastify/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node server/entry.fastify",
    "serve:build": "pnpm build && pnpm serve",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "clean": "rm -rf dist && rm -rf server && rm -rf build"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.4.5",
    "@builder.io/qwik-city": "^1.4.5",
    "@fastify/compress": "^6.5.0",
    "@fastify/static": "^6.12.0",
    "@types/eslint": "^8.56.5",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.24",
    "@types/nodemailer": "^6.4.14",
    "@types/randomstring": "^1.1.12",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "autoprefixer": "^10.4.17",
    "daisyui": "^3.9.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qwik": "^1.4.5",
    "fastify": "^4.26.1",
    "fastify-plugin": "^4.5.1",
    "postcss": "^8.4.35",
    "prettier": "2.8.8",
    "prisma": "5.0.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^3.14.0",
    "typescript": "^5.3.3",
    "undici": "^5.28.3",
    "vite": "^4.5.2",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "prisma": {
    "seed": "tsx src/db/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "5.0.0",
    "classnames": "^2.5.1",
    "date-fns": "^2.30.0",
    "jsonwebtoken": "^9.0.2",
    "lru-cache": "^9.1.2",
    "nodemailer": "^6.9.11",
    "qwik-ionicons": "^1.0.5",
    "randomstring": "^1.3.0",
    "zod": "^3.22.4"
  }
}
